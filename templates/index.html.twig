{% set title = 'Supply Request Form' %}
{% extends [settings.template.page, 'page.html.twig'] %}

{% block content %}
{{ parent() }}
    <form method="post">
        <fieldset class="form-horizontal">
            <legend>Information</legend>
            <div class="form-group">
                <label for="department" class="col-sm-2 control-label">Department</label>
                <div class="col-sm-6">
                    {{
                        helper.input({
                            type: 'text',
                            name: 'department',
                            value: department,
                            attribs: {
                                class: 'form-control',
                                id: 'department'
                            }
                        })
                        |raw
                    }}
                </div>
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2 control-label">Name</label>
                <div class="col-sm-6">
                    {{
                        helper.input({
                            type: 'text',
                            name: 'name',
                            value: name,
                            attribs: {
                                class: 'form-control',
                                id: 'name'
                            }
                        })
                        |raw
                    }}
                </div>
            </div>
            <div class="form-group">
                <label for="email" class="col-sm-2 control-label">Email</label>
                <div class="col-sm-6">
                    {{
                        helper.input({
                            type: 'email',
                            name: 'email',
                            value: email,
                            attribs: {
                                class: 'form-control',
                                id: 'email',
                                required: true
                            }
                        })
                        |raw
                    }}
                </div>
                <div class="col-sm-4 help-block">
                    <strong class="text-danger">* Required</strong>
                </div>
            </div>
            <div class="form-group">
                <label for="phone" class="col-sm-2 control-label">Phone</label>
                <div class="col-sm-6">
                    {{
                        helper.input({
                            type: 'tel',
                            name: 'phone',
                            value: phone,
                            attribs: {
                                class: 'form-control',
                                id: 'phone'
                            }
                        })
                        |raw
                    }}
                </div>
            </div>
        </fieldset>
