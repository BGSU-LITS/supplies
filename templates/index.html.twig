                                    id: 'items_miscellaneous_scissors'
                                }
                            })
                            |raw
                        }}
                    </td>
                </tr>
                <tr>
                    <td>
                        <label for="items_miscellaneous_thermal_receipt_paper">
                            Thermal Receipt Paper
                        </label>
                    </td>
                    <td>
                        {{
                            helper.input({
                                type: 'text',
                                name: 'items[Miscellaneous - Thermal Receipt Paper][Quantity]',
                                value: items['Miscellaneous - Thermal Receipt Paper']['Quantity'],
                                attribs: {
                                    class: 'form-control',
                                    id: 'items_miscellaneous_thermal_receipt_paper'
                                }
                            })
                            |raw
                        }}
                    </td>
                </tr>
                <tr>
                    <td>
                        <label for="items_miscellaneous_tissue_box">
                            Tissue Box
                        </label>
                    </td>
                    <td>
                        {{
                            helper.input({
                                type: 'text',
                                name: 'items[Miscellaneous - Tissue Box][Quantity]',
                                value: items['Miscellaneous - Tissue Box']['Quantity'],
                                attribs: {
                                    class: 'form-control',
                                    id: 'items_miscellaneous_tissue_box'
                                }
                            })
                            |raw
                        }}
                    </td>
                </tr>
            </tbody>
        </table>
        <fieldset class="form">
            <legend>Other Supplies</legend>
            <div class="form-group">
                <label for="items_other_supplies">
                    Please use the space below to detail any additional items needed. Include essential information such as vendor name, description, quantity, color, size, etc. If necessary, forward the information on a separate sheet of paper to the Budget Office.
                </label>
                {{
                    helper.input({
                        type: 'textarea',
                        name: 'items[Other Supplies]',
                        value: items['Other Supplies'],
                        attribs: {
                            class: 'form-control',
                            id: 'items_other_supplies'
                        }
                    })
                    |raw
                }}
            </div>
        </fieldset>
        {{
            helper.input({
                type: 'submit',
                name: false,
                value: 'Email Your Request',
                attribs: {class: 'btn btn-primary'}
            })
            |raw
        }}
        {{ csrf_hidden|raw }}
    </form>
{% endblock %}

{% block styles %}
{{ parent() }}
    <style>
        label { font-weight: normal; }
        .help-block { margin: 7px 0 0 0; }
        .table-bordered, .table-bordered th, .table-bordered td { border: 1px solid #bbb !important; }
        .table-bordered thead th { background: #ff9; text-align: center; }
        .table-bordered tbody th { background: #fc6; }
        .table-bordered tbody td { background: #ffc; }
        .table-bordered tbody td[colspan] { background: #ffff9e; }
        .table-bordered tbody td.indent { padding-left: 3em; }
        .table-bordered label { margin: 6px 0; }
    </style>
{% endblock %}

{% block scripts %}
{{ parent() }}
    <script src="/assets/js/jquery.floatThead.js"></script>
    <script>
    $('.table-bordered').floatThead({
        getSizingRow: function($table) {
            return $table.find('tbody tr:visible:nth-child(2)>*');
        }
    });
    </script>
{% endblock %}
